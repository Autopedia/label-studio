{
    "title": "Autopedia Label Studio",
    "protocol": "http://",
    "host": "0.0.0.0",
    "port": 8080,
    "debug": false,
    "input_path": null,
    "output_dir": "completions",
    "instruction": "이 사이트는 차량 경고등을 라벨링하기 위한 사이트입니다.",
    "allow_delete_completions": false,
    "templates_dir": "examples",
    "editor": {
        "debug": false
    },
    "sampling": "uniform",
    "ml_backends": [],
    "source": {
        "name": "source",
        "type": "s3",
        "path": "text-header-dataset",
        "params": {
            "prefix": "images1",
            "regex": "^.*\\.(jpeg|png|JPG|gif|GIF|doc|DOC|pdf|PDF)$",
            "data_key": "image",
            "use_blob_urls": true
        }
    },
    "target": {
        "name": "target",
        "type": "s3-completions",
        "path": "text-header-dataset",
        "params": {
            "prefix": "outputs1",
            "create_local_copy": false
        }
    },
    "label_config_updated": true
}